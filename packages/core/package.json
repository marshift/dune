{
	"name": "@dunejs/core",
	"version": "0.1.0",
	"description": "templating engine and static site generator using [KDL](//kdl.dev)",
	"exports": "./src/mod.ts",
	"type": "module",
	"scripts": {
		"build": "esbuild ./src/mod.ts --outdir=./dist/ --format=esm --minify --bundle"
	},
	"dependencies": {
		"jsep": "^1.4.0",
		"kdljs": "^0.3.0",
		"sucrase": "^3.35.0"
	},
	"devDependencies": {
		"esbuild": "^0.25.3"
	},
	"imports": {
		"#lib/remote/fetch": {
			"deno": "./src/lib/remote/fetch/web.ts",
			"bun": "./src/lib/remote/fetch/web.ts",
			"node": "./src/lib/remote/fetch/node.ts",
			"default": "./src/lib/remote/fetch/web.ts"
		},
		"#lib/remote/import": {
			"deno": "./src/lib/remote/import/vanilla.ts",
			"bun": "./src/lib/remote/import/vanilla.ts",
			"node": "./src/lib/remote/import/uri.ts",
			"default": "./src/lib/remote/import/uri.ts"
		}
	}
}
